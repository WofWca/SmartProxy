{
	"name": "Обойти замедление YouTube",
	"version": "1.5",
	"manifest_version": 3,
	"author": "Salar Khalilzadeh",
	"description": "Обойдите замедление YouTube и ускорьте Ютуб в России. Наслаждайтесь высоким качеством видео без задержек и буферизации.",
	"homepage_url": "https://github.com/salarcode/SmartProxy",
	"default_locale": "en",
	"icons": {
		"16": "icons/smartproxy-16.png",
		"24": "icons/smartproxy-24.png",
		"48": "icons/smartproxy-48.png",
		"96": "icons/smartproxy-96.png",
		"128": "icons/smartproxy-128.png"
	},
	"permissions": [
		"proxy",
		"webRequest",
		"webRequestAuthProvider",
		"storage"
	],
	"host_permissions": [
		"https://www.youtube.com/"
	],
	"action": {
		"default_icon": {
			"16": "icons/smartproxy-16.png",
			"24": "icons/smartproxy-24.png",
			"48": "icons/smartproxy-48.png",
			"96": "icons/smartproxy-96.png",
			"128": "icons/smartproxy-128.png"
		}
	},
	"background": {
		"service_worker": "core.js"
	},
	"minimum_chrome_version": "100"
}