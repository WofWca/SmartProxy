{
	"name": "Обойти замедление YouTube. Ускорить Ютуб в России.",
	"version": "1.5.1",
	"manifest_version": 3,
	"author": "Salar Khalilzadeh",
	"description": "Обойдите замедление YouTube и ускорьте Ютуб в России. Наслаждайтесь высокой скоростью, как раньше, до замедления.",
	"homepage_url": "https://github.com/salarcode/SmartProxy",
	"default_locale": "en",
	"icons": {
		"16": "icons/speed-16-16.png",
		"32": "icons/speed-32-32.png",
		"128": "icons/speed-128-128.png",
		"512": "icons/speed-512-512.png"
	},
	"permissions": [
		"proxy",
		"webRequest",
		"webRequestAuthProvider",
		"storage"
	],
	"host_permissions": [
		"https://www.youtube.com/"
	],
	"action": {
		"default_icon": {
			"16": "icons/speed-16-16.png",
			"32": "icons/speed-32-32.png",
			"128": "icons/speed-128-128.png",
			"512": "icons/speed-512-512.png"
		}
	},
	"background": {
		"service_worker": "core.js"
	},
	"minimum_chrome_version": "100"
}